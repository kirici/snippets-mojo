fn fizzbuzz(num: Int) -> String:
    var res: String = ""
    for n in range(1, num + 1):
        if n % 3 == 0:
            res += "Fizz"
        if n % 5 == 0:
            res += "Buzz"
        if n % 5 != 0 and n % 3 != 0:
            res += String(n)
        res += "\n"
    return res


fn main():
    print("Hello World!")
    let num = 45000
    let res = fizzbuzz(num)
    print(res)
